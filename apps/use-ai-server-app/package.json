{
  "name": "@meetsmore-oss/use-ai-server-app",
  "version": "1.0.0",
  "author": {
    "name": "Zachary Davison",
    "url": "https://github.com/mm-zacharydavison"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meetsmore/use-ai.git",
    "directory": "apps/use-ai-server-app"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
