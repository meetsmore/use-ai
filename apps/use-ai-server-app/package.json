    "dev": "bun --watch src/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.0-beta.59",
    "@ai-sdk/openai": "3.0.0-beta.66",
    "@meetsmore/use-ai-server": "workspace:*",
    "@meetsmore/use-ai-plugin-workflows": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}